var
    a, b, mx : integer;

procedure proc1(e, f: integer);
begin
end;

function max(a, b, c: integer): integer;
begin
    if a > b then mx := a else mx := b;
    if c > mx then mx := c;
    max := mx;
end;

function nothing: integer;
begin
    nothing := 0;
end;

